                                android:paddingEnd="8sp"
                                app:layout_constrainedHeight="true"
                                app:layout_constrainedWidth="true"
                                android:fontFamily="@font/roboto_light"
                                android:text="UMUR"
                                android:textColor="#000"
                                android:autoSizeTextType="uniform"
                                android:autoSizeMaxTextSize="30sp"
                                android:autoSizeMinTextSize="10sp"
                                app:layout_constraintTop_toBottomOf="@id/cdhor9"
                                app:layout_constraintLeft_toLeftOf="@id/ageEdtx"
                                app:layout_constraintBottom_toTopOf="@id/cdhor10"
                                app:layout_constraintRight_toRightOf="@id/ageEdtx" />

                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor11"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.345" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor12"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.41" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdver3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                app:layout_constraintGuide_percent="0.45" />

                            <EditText
                                android:id="@+id/ageEdtx"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:maxLength="2"
                                android:gravity="center"
                                app:layout_constrainedWidth="true"
                                android:autofillHints="email"
                                android:background="@drawable/borderblue"
                                android:fontFamily="@font/helvetica_light"
                                android:paddingVertical="2dp"
                                android:textSize="16sp"
                                android:paddingHorizontal="8sp"
                                android:inputType="number"
                                app:layout_constrainedHeight="true"
                                app:layout_constraintTop_toBottomOf="@id/cdhor11"
                                app:layout_constraintLeft_toRightOf="@id/cdver1"
                                app:layout_constraintRight_toLeftOf="@id/cdver3"
                                app:layout_constraintBottom_toTopOf="@id/cdhor12"/>
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdver4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                app:layout_constraintGuide_percent="0.50" />
                            <TextView
                                android:id="@+id/genderTV"
                                android:labelFor="@id/genderEdtx"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:paddingStart="8sp"
                                android:paddingEnd="8sp"
                                app:layout_constrainedHeight="true"
                                app:layout_constrainedWidth="true"
                                android:fontFamily="@font/roboto_light"
                                android:text="GENDER"
                                android:textColor="#000"
                                android:autoSizeTextType="uniform"
                                android:autoSizeMaxTextSize="30sp"
                                android:autoSizeMinTextSize="10sp"
                                app:layout_constraintTop_toBottomOf="@id/cdhor9"
                                app:layout_constraintLeft_toLeftOf="@id/genderEdtx"
                                app:layout_constraintBottom_toTopOf="@id/cdhor10"
                                app:layout_constraintRight_toRightOf="@id/genderEdtx" />
                            <RadioGroup
                                android:id="@+id/genderEdtx"
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:orientation="horizontal"
                                app:layout_constrainedWidth="true"
                                app:layout_constrainedHeight="true"
                                app:layout_constraintTop_toBottomOf="@id/cdhor11"
                                app:layout_constraintBottom_toTopOf="@id/cdhor12"
                                app:layout_constraintRight_toRightOf="@id/cdver2">
                                <androidx.appcompat.widget.AppCompatRadioButton
                                    android:id="@+id/radio0"
                                    android:gravity="center"
                                    android:layout_width="wrap_content"
                                    android:minWidth="55dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginRight="5dp"
                                    android:background="@drawable/cstmradio"
                                    android:button="@drawable/cstmradio"
                                    android:text="L" />
                                <androidx.appcompat.widget.AppCompatRadioButton
                                    android:gravity="center"
                                    android:id="@+id/radio1"
                                    android:layout_width="wrap_content"
                                    android:minWidth="55dp"
                                    android:layout_marginLeft="5dp"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/cstmradio"
                                    android:button="@drawable/cstmradio"
                                    android:text="P" />
                            </RadioGroup>
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor13"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.425" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor14"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.46" />
                            <TextView
                                android:id="@+id/phoneTV"
                                android:labelFor="@id/phoneEdtx"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:paddingStart="8sp"
                                android:paddingEnd="8sp"
                                app:layout_constrainedHeight="true"
                                app:layout_constrainedWidth="true"
                                android:fontFamily="@font/roboto_light"
                                android:text="TELEPON"
                                android:textColor="#000"
                                android:autoSizeTextType="uniform"
                                android:autoSizeMaxTextSize="30sp"
                                android:autoSizeMinTextSize="10sp"
                                app:layout_constraintTop_toBottomOf="@id/cdhor13"
                                app:layout_constraintLeft_toRightOf="@id/cdver1"
                                app:layout_constraintBottom_toTopOf="@id/cdhor14"
                                app:layout_constraintRight_toRightOf="@id/cdver2" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor15"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.475" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor16"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.54" />
                            <EditText
                                android:id="@+id/phoneEdtx"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                app:layout_constrainedWidth="true"
                                android:autofillHints="email"
                                android:background="@drawable/borderblue"
                                android:fontFamily="@font/helvetica_light"
                                android:paddingVertical="2dp"
                                android:textSize="16sp"
                                android:paddingHorizontal="8sp"
                                android:inputType="phone"
                                app:layout_constrainedHeight="true"
                                app:layout_constraintTop_toBottomOf="@id/cdhor15"
                                app:layout_constraintLeft_toRightOf="@id/cdver1"
                                app:layout_constraintRight_toLeftOf="@id/cdver2"
                                app:layout_constraintBottom_toTopOf="@id/cdhor16"/>

                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor17"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.555" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor18"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.59" />
                            <TextView
                                android:id="@+id/addTV"
                                android:labelFor="@id/addEdtx"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:paddingStart="8sp"
                                android:paddingEnd="8sp"
                                app:layout_constrainedHeight="true"
                                app:layout_constrainedWidth="true"
                                android:fontFamily="@font/roboto_light"
                                android:text="ALAMAT"
                                android:textColor="#000"
                                android:autoSizeTextType="uniform"
                                android:autoSizeMaxTextSize="30sp"
                                android:autoSizeMinTextSize="10sp"
                                app:layout_constraintTop_toBottomOf="@id/cdhor17"
                                app:layout_constraintLeft_toRightOf="@id/cdver1"
                                app:layout_constraintBottom_toTopOf="@id/cdhor18"
                                app:layout_constraintRight_toRightOf="@id/cdver2" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor19"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.605" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor20"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
