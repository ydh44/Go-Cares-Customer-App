<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cons"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="15dp"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintWidth_default="percent">

            <RelativeLayout
                android:id="@+id/progressbar"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:translationZ="20dp"
                android:elevation="30dp"
                android:visibility="gone"
                android:background="@drawable/backbluebig3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >
                <androidx.core.widget.ContentLoadingProgressBar
                    android:id="@+id/prgs"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="15dp"
                    style="?android:attr/progressBarStyleLarge"/>
            </RelativeLayout>


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/backhor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.65" />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginBottom="8pt"
                android:background="@drawable/backbluebig"
                android:elevation="4dp"
                app:layout_constraintBottom_toTopOf="@id/backhor"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/cardhor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.1" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/cardhor2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.99" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/cardver1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.08" />
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/cardver2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.92" />
            <androidx.cardview.widget.CardView
                android:id="@+id/cv2"
                android:layout_height="0dp"
                android:layout_width="0dp"
                app:layout_constraintDimensionRatio="1:1.7"
                android:maxWidth="350dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="4dp"
                android:layout_marginBottom="15dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="@id/cardhor1"
                app:layout_constraintLeft_toRightOf="@id/cardver1"
                app:layout_constraintRight_toLeftOf="@id/cardver2">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.cardview.widget.CardView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:cardCornerRadius="15dp"
                        app:layout_constraintDimensionRatio="1:1.7"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent">
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdver2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                app:layout_constraintGuide_percent="0.93" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdver1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                app:layout_constraintGuide_percent="0.07" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.035" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.070" />
                            <TextView
                                android:id="@+id/emailTV"
                                android:labelFor="@id/emailEdtx"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:paddingStart="8sp"
                                android:paddingEnd="8sp"
                                app:layout_constrainedHeight="true"
                                app:layout_constrainedWidth="true"
                                android:fontFamily="@font/roboto_light"
                                android:text="@string/email"
                                android:textColor="#000"
                                android:autoSizeTextType="uniform"
                                android:autoSizeMaxTextSize="30sp"
                                android:autoSizeMinTextSize="10sp"
                                app:layout_constraintTop_toBottomOf="@id/cdhor1"
                                app:layout_constraintLeft_toRightOf="@id/cdver1"
                                app:layout_constraintBottom_toTopOf="@id/cdhor2"
                                app:layout_constraintRight_toRightOf="@id/cdver2" />

                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.085" />

                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.15" />
                            <EditText
                                android:id="@+id/emailEdtx"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                app:layout_constrainedWidth="true"
                                android:autofillHints="email"
                                android:background="@drawable/borderblue"
                                android:fontFamily="@font/helvetica_light"
                                android:paddingVertical="2dp"
                                android:textSize="16sp"
                                android:paddingHorizontal="8sp"
                                android:inputType="textEmailAddress"
                                app:layout_constrainedHeight="true"
                                app:layout_constraintTop_toBottomOf="@id/cdhor3"
                                app:layout_constraintLeft_toRightOf="@id/cdver1"
                                app:layout_constraintRight_toLeftOf="@id/cdver2"
                                app:layout_constraintBottom_toTopOf="@id/cdhor4"/>

                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.165" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.20" />

                            <TextView
                                android:id="@+id/nameTV"
                                android:labelFor="@id/nameEdtx"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:paddingStart="8sp"
                                android:paddingEnd="8sp"
                                app:layout_constrainedHeight="true"
                                app:layout_constrainedWidth="true"
                                android:fontFamily="@font/roboto_light"
                                android:text="NAMA"
                                android:textColor="#000"
                                android:autoSizeTextType="uniform"
                                android:autoSizeMaxTextSize="30sp"
                                android:autoSizeMinTextSize="10sp"
                                app:layout_constraintTop_toBottomOf="@id/cdhor5"
                                app:layout_constraintLeft_toRightOf="@id/cdver1"
                                app:layout_constraintBottom_toTopOf="@id/cdhor6"
                                app:layout_constraintRight_toRightOf="@id/cdver2" />

                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.215" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor8"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                app:layout_constraintGuide_percent="0.28" />

                            <EditText
                                android:id="@+id/nameEdtx"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                app:layout_constrainedWidth="true"
                                android:autofillHints="email"
