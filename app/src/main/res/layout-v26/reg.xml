                                android:autoSizeMaxTextSize="30sp"
                                android:autoSizeMinTextSize="10sp"

                                app:layout_constraintTop_toBottomOf="@id/cdhor21"
                                app:layout_constraintLeft_toLeftOf="@id/pass2Edtx"
                                app:layout_constraintBottom_toTopOf="@id/cdhor22"
                                app:layout_constraintRight_toRightOf="@id/pass2Edtx" />
                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/cdhor23"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
