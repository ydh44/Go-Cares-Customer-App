<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/barhor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".07"/>

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/bar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:cardCornerRadius="0dp"
        android:background="#fff"
        app:layout_constraintHeight_min="50dp"
        app:contentInsetLeft="0dp"
        app:contentInsetStart="0dp"
        app:contentInsetStartWithNavigation="0dp"
        app:layout_constraintBottom_toTopOf="@id/barhor"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/brver1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".3"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/tilhor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".325"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/tilhor2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".655"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/loghor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".32"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/loghor2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".68"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/tilver"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".48"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/logver1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".3"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/logver2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".5"/>
            <TextView
                android:translationZ="1dp"
                android:id="@+id/txtfragment_profil"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center_vertical"
                android:text="PROFILE"
                android:textColor="#5B4949"
                app:layout_constraintHeight_min="19dp"
                android:textSize="14sp"
                android:fontFamily="@font/roboto_bold"
                app:layout_constraintTop_toBottomOf="@id/tilhor1"
                app:layout_constraintBottom_toTopOf="@id/tilhor2"
                app:layout_constraintLeft_toRightOf="@id/tilver"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.145"/>

            <ImageView
                android:id="@+id/icon"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:translationZ="1dp"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintBottom_toBottomOf="@id/loghor2"
                app:layout_constraintEnd_toEndOf="@id/logver2"
                app:layout_constraintStart_toStartOf="@id/logver1"
                app:layout_constraintTop_toTopOf="@id/loghor1"
                app:srcCompat="@drawable/profile" />
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:src="@drawable/backbluebar"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toRightOf="@id/brver1">

            </androidx.appcompat.widget.AppCompatImageView>

            <androidx.appcompat.widget.AppCompatImageButton
                android:id="@+id/backButton"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                app:layout_constraintDimensionRatio="1:1"
                android:background="@drawable/circlewhite"
                app:layout_constraintEnd_toEndOf="@id/backbuttonic"
                app:layout_constraintStart_toStartOf="@id/backbuttonic" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/backbuttonic"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="1:1"
                android:scaleType="fitCenter"
                android:src="@drawable/icbackfrag"
                android:minHeight="20dp"
                app:layout_constraintHorizontal_bias="0.3"
                app:layout_constraintBottom_toBottomOf="@id/loghor2"
                app:layout_constraintEnd_toStartOf="@+id/brver1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/loghor1" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.appcompat.widget.Toolbar>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdhor1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".12"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdhor2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".35"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdver1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".05"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdver2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".95"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toTopOf="@id/gdhor2"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintRight_toLeftOf="@id/gdver2"
        app:layout_constraintTop_toBottomOf="@id/gdhor1">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cv2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/backblue">
            <androidx.constraintlayout.widget.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/hor1"
                app:layout_constraintGuide_percent=".3"/>
            <TextView
                android:id="@+id/nama"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintWidth_percent=".9"
                android:textSize="19sp"
                android:fontFamily="@font/helvetica_medium"
                android:text="Nama cust"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/hor1"
                />
            <androidx.constraintlayout.widget.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/hor2"
                app:layout_constraintGuide_percent=".75"/>
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/buttonout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="?android:attr/borderlessButtonStyle"
                app:layout_constraintHeight_percent=".15"
                android:padding="0dp"
                android:fontFamily="@font/helvetica_bold"
                android:text="KELUAR"
                android:layout_marginRight="10dp"
                android:textColor="@color/colorRed"
                android:textSize="13sp"
                app:layout_constraintTop_toBottomOf="@id/hor2"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.4"/>
    <TextView
        android:id="@+id/a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25sp"
        android:textSize="16sp"
        android:fontFamily="@font/roboto_light"
        android:text="Email      :"
        android:textColor="@color/darkgray"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintTop_toBottomOf="@id/cdhor5" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".33"/>
    <TextView
        android:id="@+id/email"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:fontFamily="@font/roboto_light"
        android:text=""
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintLeft_toRightOf="@id/cdver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor5" />
    <TextView
        android:id="@+id/b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25sp"
        android:textSize="16sp"
        android:fontFamily="@font/roboto_light"
        android:text="Telepon  :"
        android:textColor="@color/darkgray"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintTop_toTopOf="@id/telepon" />
    <TextView
        android:id="@+id/telepon"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textSize="16sp"
        android:fontFamily="@font/roboto_light"
        android:text=""
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintLeft_toRightOf="@id/cdver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/email" />
    <TextView
        android:id="@+id/c"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25sp"
        android:textSize="16sp"
        android:fontFamily="@font/roboto_light"
        android:text="Gender   :"
        android:textColor="@color/darkgray"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintTop_toTopOf="@id/gender" />
    <TextView
        android:id="@+id/gender"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textSize="16sp"
        android:fontFamily="@font/roboto_light"
        android:text=""
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintLeft_toRightOf="@id/cdver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/telepon" />
    <TextView
        android:id="@+id/d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_marginStart="25sp"
        android:fontFamily="@font/roboto_light"
        android:text="Umur      :"
        android:textColor="@color/darkgray"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintTop_toTopOf="@id/umur" />
    <TextView
        android:id="@+id/umur"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/roboto_light"
        android:text=""
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintLeft_toRightOf="@id/cdver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/gender" />
    <TextView
        android:id="@+id/e"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_marginStart="25sp"
        android:fontFamily="@font/roboto_light"
        android:text="Alamat    :"
        android:textColor="@color/darkgray"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintTop_toTopOf="@id/alamat" />
    <TextView
        android:id="@+id/alamat"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/roboto_light"
        android:text=""
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintLeft_toRightOf="@id/cdver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/umur" />
    <TextView
        android:id="@+id/noo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:translationZ="1dp"
        android:textSize="30dp"
        android:text="No Internet"
        android:gravity="center"
        android:visibility="gone"/>

</androidx.constraintlayout.widget.ConstraintLayout>