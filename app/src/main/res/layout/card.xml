<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cardview_pesananCv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginVertical="5dp"
        app:cardCornerRadius="15dp"
        app:cardElevation="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.cardview.widget.CardView
                android:id="@+id/foto"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="10dp"
                app:cardCornerRadius="5dp"
                app:layout_constraintDimensionRatio="3:3.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/namacgTv"
                app:layout_constraintBottom_toBottomOf="@id/tanggal1Tv"
                >
                <ImageView
                    android:id="@+id/fotocg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/profilecg"
                    android:scaleType="centerCrop"/>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/namacgTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/lato"
                android:text="Nama caregiver"
                android:textColor="@color/boldblack"
                android:textSize="18sp"
                android:visibility="visible"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="10dp"
                app:layout_constraintStart_toEndOf="@id/foto"
                app:layout_constraintTop_toTopOf="parent" />


                <TextView
                    android:id="@+id/status1Tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/lato_light"
                    android:text="Selesai"
                    android:textColor="@color/mediumblack"
                    android:textSize="14sp"
                    android:visibility="visible"
                    android:layout_marginTop="15dp"
                    android:layout_marginHorizontal="10dp"
                    app:layout_constraintStart_toEndOf="@id/foto"
                    app:layout_constraintTop_toBottomOf="@id/namacgTv" />


                <TextView
                    android:id="@+id/tanggal1Tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:visibility="visible"
                    android:text="20 Juli 2020"
                    android:textColor="@color/mediumblack"
                    android:textSize="14sp"
                    android:gravity="end"
                    android:layout_marginTop="10dp"
                    android:layout_marginHorizontal="10dp"
                    android:fontFamily="@font/lato_light"
                    app:layout_constraintStart_toEndOf="@id/foto"
                    app:layout_constraintTop_toBottomOf="@id/status1Tv"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.05dp"
                app:layout_constraintBottom_toTopOf="@id/container"
                android:background="@color/lightblack"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/expandableView">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/detailBtn"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintWidth_percent="0.5"
                    android:paddingHorizontal =  "10dp"
                    android:paddingVertical="15dp">
                    <TextView
                        android:id="@+id/detailTv"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Detail"
                        android:fontFamily="@font/lato"
                        android:textColor="#2F80ED"
                        android:textSize="16sp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/actionBtn"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:clickable="true"
                    android:focusable="true"
                    app:layout_constraintWidth_percent="0.5"
                    android:background="@drawable/xmlbg10"
                    android:paddingHorizontal =  "10dp"
                    android:paddingVertical="15dp">
                    <TextView
                        android:id="@+id/actionTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Bayar"
                        android:fontFamily="@font/lato"
                        android:textColor="@color/white"
                        android:textSize="16sp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <LinearLayout
                android:id="@+id/expandableView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="15dp"
                android:orientation="vertical"
                android:visibility="gone"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@id/tanggal1Tv">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/keahlian"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <ImageView
                        android:id="@+id/keahlianLg"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintDimensionRatio="1:1"
                        android:src="@drawable/ahli"
                        android:layout_marginStart="10dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@id/keahlianTv"
                        app:layout_constraintBottom_toBottomOf="@id/keahlianTv"/>
                    <TextView
                        android:id="@+id/keahlianTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Professional Caregiver"
                        android:fontFamily="@font/lato_light"
                        android:textColor="@color/mediumblack"
                        android:textSize="14sp"
                        android:layout_marginStart="10dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toEndOf="@id/keahlianLg"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/rating"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <ImageView
                        android:id="@+id/ratingcgLg"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintDimensionRatio="1:1"
                        android:src="@drawable/star"
                        android:layout_marginStart="10dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@id/ratingcgTv"
                        app:layout_constraintBottom_toBottomOf="@id/ratingcgTv"/>
                    <TextView
                        android:id="@+id/ratingcgTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="4.0 / 5.0"
                        android:fontFamily="@font/lato_light"
                        android:textColor="@color/mediumblack"
                        android:textSize="14sp"
                        android:layout_marginStart="10dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toEndOf="@id/ratingcgLg"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <View
                    android:id="@+id/view1"
                    android:layout_marginTop="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/lightblack" />
                <LinearLayout
                    android:id="@+id/expandableView_cardview_pesanan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/detailpesananHead"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingVertical="10dp"
                        android:paddingHorizontal="10dp"
                        android:visibility="visible"
                        android:clickable="true"
                        android:focusable="true">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Detail Pesanan"
                            android:textColor="@color/mediumblack"
                            android:textSize="15sp"
                            android:fontFamily="@font/lato"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/detailpesananBtn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/ic_keyboard_arrow_down_black_24dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintDimensionRatio="1:1"
                            app:layout_constraintTop_toTopOf="parent"
                            android:scaleType="fitXY"
                            android:padding="0dp"
                            app:layout_constraintBottom_toBottomOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/expandableView_constraint_pesanan"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:paddingTop="5dp"
                        android:paddingHorizontal="10dp"
                        android:paddingBottom="20dp">

                        <TextView
                            android:id="@+id/durasiTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Durasi"
                            android:fontFamily="@font/lato_light"
                            android:textColor="@color/lightblack"
                            android:textSize="15sp"
                            app:layout_constraintTop_toTopOf="@id/durasi1Tv"
                            app:layout_constraintStart_toStartOf="parent"/>

                        <TextView
                            android:id="@+id/teleponTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Telepon"
                            android:fontFamily="@font/lato_light"
                            android:textColor="@color/lightblack"
                            android:textSize="15sp"
                            app:layout_constraintTop_toTopOf="@id/telepon1Tv"
                            app:layout_constraintStart_toStartOf="parent" />

                        <TextView
                            android:id="@+id/alamatTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Alamat"
                            android:fontFamily="@font/lato_light"
                            android:textColor="@color/lightblack"
                            android:textSize="15sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@id/alamat1Tv" />

                        <TextView
                            android:id="@+id/aktivitasTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Aktivitas"
                            android:fontFamily="@font/lato_light"
                            android:textColor="@color/lightblack"
                            android:textSize="15sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/aktivitas1Tv" />

                        <TextView
                            android:id="@+id/totalTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Total"
                            android:fontFamily="@font/lato_light"
                            android:textColor="@color/lightblack"
                            android:textSize="15sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/total1Tv" />
                        <TextView
                            android:id="@+id/tv1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:layout_marginStart="5dp"
                            app:layout_constraintStart_toEndOf="@id/aktivitasTv"
                            app:layout_constraintTop_toTopOf="@id/durasiTv"
                            app:layout_constraintBottom_toBottomOf="@id/durasiTv"
                            android:fontFamily="@font/lato_light"
                            android:textSize="16sp"
                            />

                        <TextView
                            android:id="@+id/durasi1Tv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="4 Hari"
                            android:fontFamily="@font/lato_light"
                            android:textSize="15sp"
                            android:textColor="#000"
                            android:visibility="visible"
                            android:layout_marginStart="5dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@id/tv1"
                            app:layout_constraintTop_toTopOf="parent" />
                        <TextView
                            android:id="@+id/tv2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:layout_constraintStart_toStartOf="@id/tv1"
                            app:layout_constraintTop_toTopOf="@id/teleponTv"
                            app:layout_constraintBottom_toBottomOf="@id/teleponTv"
                            android:fontFamily="@font/lato_light"
                            android:textSize="15sp"
                            />

                        <TextView
                            android:id="@+id/telepon1Tv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="089929344567"
                            android:textColor="#000"
                            android:textSize="15sp"
                            android:fontFamily="@font/lato_light"
                            android:layout_marginTop="12dp"
                            android:visibility="visible"
                            android:layout_marginStart="5dp"
                            app:layout_constraintStart_toEndOf="@id/tv2"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/durasi1Tv" />
                        <TextView
                            android:id="@+id/tv3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:layout_constraintStart_toStartOf="@id/tv1"
                            app:layout_constraintTop_toTopOf="@id/alamatTv"
                            app:layout_constraintBottom_toBottomOf="@id/alamatTv"
                            android:fontFamily="@font/lato_light"
                            android:textSize="15sp"
                            />

                        <TextView
                            android:id="@+id/alamat1Tv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Jalan Cempaka Putih no 12"
                            android:textColor="#000"
                            android:textSize="15sp"
                            android:layout_marginStart="5dp"
                            android:layout_marginTop="12dp"
                            android:fontFamily="@font/lato_light"
                            app:layout_constraintStart_toEndOf="@id/tv3"
                            app:layout_constraintTop_toBottomOf="@id/telepon1Tv"
                            app:layout_constraintEnd_toEndOf="parent" />
                        <TextView
                            android:id="@+id/tv4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:layout_constraintStart_toStartOf="@id/tv1"
                            app:layout_constraintTop_toTopOf="@id/aktivitasTv"
                            app:layout_constraintBottom_toBottomOf="@id/aktivitasTv"
                            android:fontFamily="@font/lato_light"
                            android:textSize="15sp"
                            />

                        <TextView
                            android:id="@+id/aktivitas1Tv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Senang - senang"
                            android:textColor="#000"
                            android:textSize="15sp"
                            android:layout_marginStart="5dp"
                            android:layout_marginTop="12dp"
                            android:fontFamily="@font/lato_light"
                            app:layout_constraintStart_toEndOf="@id/tv4"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/alamat1Tv" />
                        <TextView
                            android:id="@+id/tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:layout_constraintStart_toStartOf="@id/tv1"
                            app:layout_constraintTop_toTopOf="@id/totalTv"
                            app:layout_constraintBottom_toBottomOf="@id/totalTv"
                            android:fontFamily="@font/lato_light"
                            android:textSize="15sp"
                            />

                        <TextView
                            android:id="@+id/total1Tv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Rp 400, 000"
                            android:textColor="#000000"
                            android:textSize="15sp"
                            android:layout_marginStart="5dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="15dp"
                            android:fontFamily="@font/lato_light"
                            app:layout_constraintStart_toEndOf="@id/tv5"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/aktivitas1Tv" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>
                <View
                    android:id="@+id/view2"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/lightblack" />

                <LinearLayout
                    android:id="@+id/expandableView_cardview_lansia"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="visible">
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/detaillansiaHead"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingVertical="10dp"
                        android:paddingHorizontal="10dp"
                        android:clickable="true"
                        android:focusable="true">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Detail Lansia"
                            android:textColor="@color/mediumblack"
                            android:textSize="15sp"
                            android:fontFamily="@font/lato"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/detaillansiaBtn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/ic_keyboard_arrow_down_black_24dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintDimensionRatio="1:1"
                            app:layout_constraintTop_toTopOf="parent"
                            android:scaleType="fitXY"
                            android:padding="0dp"
                            app:layout_constraintBottom_toBottomOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/expandableView_constraint_lansia"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:paddingTop="5dp"
                        android:paddingHorizontal="10dp"
                        android:paddingBottom="20dp">

                        <TextView
                            android:id="@+id/namaTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nama"
                            android:fontFamily="@font/lato_light"
                            android:textColor="@color/lightblack"
                            android:textSize="15sp"
                            app:layout_constraintTop_toTopOf="@id/nama1Tv"
                            app:layout_constraintStart_toStartOf="parent"/>

                        <TextView
                            android:id="@+id/umurTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Umur"
                            android:fontFamily="@font/lato_light"
                            android:textColor="@color/lightblack"
                            android:textSize="15sp"
                            app:layout_constraintTop_toTopOf="@id/umur1Tv"
                            app:layout_constraintStart_toStartOf="parent" />

                        <TextView
                            android:id="@+id/genderTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Gender"
                            android:fontFamily="@font/lato_light"
                            android:textColor="@color/lightblack"
                            android:textSize="15sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@id/gender1Tv" />

                        <TextView
                            android:id="@+id/hobiTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Hobi"
                            android:fontFamily="@font/lato_light"
                            android:textColor="@color/lightblack"
                            android:textSize="15sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/hobi1Tv" />

                        <TextView
                            android:id="@+id/riwayatTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Riwayat"
                            android:fontFamily="@font/lato_light"
                            android:textColor="@color/lightblack"
                            android:textSize="15sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/riwayat1Tv" />
                        <TextView
                            android:id="@+id/tv6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:layout_marginStart="5dp"
                            app:layout_constraintStart_toEndOf="@id/riwayatTv"
                            app:layout_constraintTop_toTopOf="@id/namaTv"
                            app:layout_constraintBottom_toBottomOf="@id/namaTv"
                            android:fontFamily="@font/lato_light"
                            android:textSize="16sp"
                            />

                        <TextView
                            android:id="@+id/nama1Tv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="4 Hari"
                            android:fontFamily="@font/lato_light"
                            android:textSize="15sp"
                            android:textColor="#000"
                            android:visibility="visible"
                            android:layout_marginStart="5dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@id/tv6"
                            app:layout_constraintTop_toTopOf="parent" />
                        <TextView
                            android:id="@+id/tv7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:layout_constraintStart_toStartOf="@id/tv6"
                            app:layout_constraintTop_toTopOf="@id/umurTv"
                            app:layout_constraintBottom_toBottomOf="@id/umurTv"
                            android:fontFamily="@font/lato_light"
                            android:textSize="15sp"
                            />

                        <TextView
                            android:id="@+id/umur1Tv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="089929344567"
                            android:textColor="#000"
                            android:textSize="15sp"
                            android:fontFamily="@font/lato_light"
                            android:layout_marginTop="12dp"
                            android:visibility="visible"
                            android:layout_marginStart="5dp"
                            app:layout_constraintStart_toEndOf="@id/tv7"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/nama1Tv" />
                        <TextView
                            android:id="@+id/tv8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:layout_constraintStart_toStartOf="@id/tv6"
                            app:layout_constraintTop_toTopOf="@id/genderTv"
                            app:layout_constraintBottom_toBottomOf="@id/genderTv"
                            android:fontFamily="@font/lato_light"
                            android:textSize="15sp"
                            />

                        <TextView
                            android:id="@+id/gender1Tv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Jalan Cempaka Putih no 12"
                            android:textColor="#000"
                            android:textSize="15sp"
                            android:layout_marginStart="5dp"
                            android:layout_marginTop="12dp"
                            android:fontFamily="@font/lato_light"
                            app:layout_constraintStart_toEndOf="@id/tv7"
                            app:layout_constraintTop_toBottomOf="@id/umur1Tv"
