<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/barhor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".08"/>

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/bar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:cardCornerRadius="0dp"
        android:background="#fff"
        app:layout_constraintHeight_min="50dp"
        app:layout_constraintBottom_toTopOf="@id/barhor"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/brver1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".3"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/tilhor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".325"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/tilhor2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".655"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/loghor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".32"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/loghor2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".68"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/tilver"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".48"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/logver1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".3"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/logver2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".5"/>
            <TextView
                android:translationZ="1dp"
                android:id="@+id/txtfragment_profil"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:gravity="center_vertical"
                android:text="PROFILE"
                android:textColor="#5B4949"
                app:layout_constraintHeight_min="19dp"
                android:autoSizeMaxTextSize="40sp"
                android:autoSizeMinTextSize="10sp"
                android:autoSizeTextType="uniform"
                android:fontFamily="@font/roboto_bold"
                app:layout_constraintTop_toBottomOf="@id/tilhor1"
                app:layout_constraintBottom_toTopOf="@id/tilhor2"
                app:layout_constraintLeft_toRightOf="@id/tilver"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.145"/>

            <ImageView
                android:id="@+id/icon"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:translationZ="1dp"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintBottom_toBottomOf="@id/loghor2"
                app:layout_constraintEnd_toEndOf="@id/logver2"
                app:layout_constraintStart_toStartOf="@id/logver1"
                app:layout_constraintTop_toTopOf="@id/loghor1"
                app:srcCompat="@drawable/profile" />
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:src="@drawable/backbluebar"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toRightOf="@id/brver1">

            </androidx.appcompat.widget.AppCompatImageView>

            <androidx.appcompat.widget.AppCompatImageButton
                android:id="@+id/backButton"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="1:1"
                android:background="@drawable/circlewhite"
                app:layout_constraintWidth_percent=".15"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@id/backbuttonic"
                app:layout_constraintStart_toStartOf="@id/backbuttonic"
                app:layout_constraintTop_toTopOf="parent" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/backbuttonic"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="1:1"
                android:scaleType="fitCenter"
                android:src="@drawable/icbackfrag"
                android:minHeight="20dp"
                app:layout_constraintHorizontal_bias="0.3"
                app:layout_constraintBottom_toBottomOf="@id/loghor2"
                app:layout_constraintEnd_toStartOf="@+id/brver1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/loghor1" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.appbar.AppBarLayout>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdhor1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".12"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdhor2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".35"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdver1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".05"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gdver2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".95"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toTopOf="@id/gdhor2"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintRight_toLeftOf="@id/gdver2"
        app:layout_constraintTop_toBottomOf="@id/gdhor1">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/backblue">
            <androidx.constraintlayout.widget.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/hor1"
                app:layout_constraintGuide_percent=".3"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintWidth_percent=".9"
                android:autoSizeMaxTextSize="25sp"
                android:autoSizeMinTextSize="10sp"
                android:autoSizeTextType="uniform"
                android:fontFamily="@font/helvetica_medium"
                android:text="Nama cust"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/hor1"
                app:layout_constraintHeight_percent=".15"
                />
            <androidx.constraintlayout.widget.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/hor2"
                app:layout_constraintGuide_percent=".75"/>
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/buttonout"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                style="?android:attr/borderlessButtonStyle"
                app:layout_constraintHeight_percent=".1"
                android:padding="0dp"
                android:fontFamily="@font/helvetica_bold"
                android:text="KELUAR"
                android:layout_marginRight="10dp"
                android:textColor="@color/colorRed"
                android:autoSizeTextType="uniform"
                android:autoSizeMaxTextSize="20sp"
                android:autoSizeMinTextSize="6sp"
                app:layout_constraintTop_toBottomOf="@id/hor2"
                app:layout_constraintRight_toRightOf="parent" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.4"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.4275"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="25sp"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/helvetica_light"
        android:text="Email      :"
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/cdhor6"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor5" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".33"/>
    <TextView
        android:id="@+id/kota"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/helvetica_thin"
        android:text="Semarang"
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/cdhor6"
        app:layout_constraintLeft_toRightOf="@id/cdver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor5" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.455"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.4825"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="25sp"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/helvetica_light"
        android:text="Telepon :"
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/cdhor8"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor7" />
    <TextView
        android:id="@+id/umur"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/helvetica_thin"
        android:text="Umur"
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/cdhor8"
        app:layout_constraintLeft_toRightOf="@id/cdver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor7" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.51"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5375"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="25sp"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/helvetica_light"
        android:text="Gender  :"
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/cdhor10"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor9" />
    <TextView
        android:id="@+id/gender"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/helvetica_thin"
        android:text="Gender"
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/cdhor10"
        app:layout_constraintLeft_toRightOf="@id/cdver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor9" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.565"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5925"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:layout_marginStart="25sp"
        android:fontFamily="@font/helvetica_light"
        android:text="Umur     :"
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/cdhor12"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor11" />
    <TextView
        android:id="@+id/gaji"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/helvetica_thin"
        android:text="Gaji"
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/cdhor12"
        app:layout_constraintLeft_toRightOf="@id/cdver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor11" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.62"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cdhor14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.6475"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:layout_marginStart="25sp"
        android:fontFamily="@font/helvetica_light"
        android:text="Alamat   :"
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/cdhor14"
        app:layout_constraintLeft_toRightOf="@id/gdver1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor13" />
    <TextView
        android:id="@+id/rating"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/helvetica_thin"
        android:text="Rating"
        android:textColor="#000000"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/cdhor14"
        app:layout_constraintLeft_toRightOf="@id/cdver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cdhor13" />

</androidx.constraintlayout.widget.ConstraintLayout>