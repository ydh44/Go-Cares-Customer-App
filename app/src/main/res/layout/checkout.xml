<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintHeight_default="percent"
    app:layout_constraintWidth_default="percent">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/btnhor1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.03" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/btnhor2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.085"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/btn2hor1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.0375" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/btn2hor2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.0775"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cardhor1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.15" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cardhor2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.925"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cardver1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.08" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/cardver2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.92" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:translationZ="3dp"
        app:layout_constraintTop_toBottomOf="@id/btn2hor1"
        app:layout_constraintLeft_toLeftOf="@id/btnBack"
        app:layout_constraintBottom_toTopOf="@id/btn2hor2"
        app:layout_constraintRight_toRightOf="@id/btnBack">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintDimensionRatio="1:1"
            android:layout_marginEnd="30dp"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginVertical="2dp"
            app:srcCompat="@drawable/back"
            app:layout_constrainedWidth="true"
            app:layout_constrainedHeight="true"
            />
    </LinearLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnBack"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintWidth_max="120dp"
        app:layout_constraintWidth_percent=".2"
        android:background="@drawable/backblue2"
        android:fontFamily="@font/roboto_bold"
        android:textColor="#FFFFFF"
        android:autoSizeTextType="uniform"
        android:autoSizeMaxTextSize="16sp"
        android:autoSizeMinTextSize="8sp"
        app:layout_constrainedWidth="true"
        app:layout_constrainedHeight="true"
        app:layout_constraintTop_toBottomOf="@id/btnhor1"
        app:layout_constraintLeft_toLeftOf="@id/cv2"
        app:layout_constraintBottom_toTopOf="@id/btnhor2"
        app:layout_constraintRight_toRightOf="@id/cv2"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="CHECKOUT"
        android:layout_marginLeft="15dp"
        app:layout_constraintLeft_toRightOf="@id/btnBack"
        app:layout_constraintTop_toBottomOf="@id/btn2hor1"
        app:layout_constraintBottom_toTopOf="@id/btn2hor2"
        app:layout_constraintRight_toLeftOf="@id/cardver2"
        android:layout_marginVertical="0dp"
        android:textColor="#000"
        android:autoSizeTextType="uniform"
        android:autoSizeMaxTextSize="40sp"
        android:autoSizeMinTextSize="12sp"
        app:layout_constrainedWidth="true"
        app:layout_constrainedHeight="true"
        android:fontFamily="@font/roboto_bold"/>
    <androidx.cardview.widget.CardView
        android:id="@+id/cv2"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintDimensionRatio="1:1.5"
        android:maxHeight="20dp"
        android:maxWidth="350dp"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:cardCornerRadius="15dp"
        app:cardElevation="4dp"
        app:layout_constraintTop_toBottomOf="@id/cardhor1"
        app:layout_constraintLeft_toRightOf="@id/cardver1"
        app:layout_constraintRight_toLeftOf="@id/cardver2"
        app:layout_constraintBottom_toTopOf="@id/cardhor2">
        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="1:2.4"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent">
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cardbghor1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.475" />
                        <RelativeLayout
                            android:id="@+id/bgcard"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:background="@drawable/backbluebigmed"
                            android:layout_marginEnd="15dp"
                            app:layout_constraintBottom_toBottomOf="@id/cardbghor1"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/tithor1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.015" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/tithor2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.045"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:autoSizeMaxTextSize="40sp"
                            android:autoSizeMinTextSize="8sp"
                            android:autoSizeTextType="uniform"
                            android:fontFamily="@font/helvetica_light"
                            android:text="Data Lansia"
                            android:textAlignment="center"
                            android:textColor="#fff"
                            app:layout_constrainedHeight="true"
                            app:layout_constrainedWidth="true"
                            app:layout_constraintBottom_toTopOf="@id/tithor2"
                            app:layout_constraintLeft_toLeftOf="@id/bgcard"
                            app:layout_constraintRight_toRightOf="@id/bgcard"
                            app:layout_constraintTop_toBottomOf="@id/tithor1" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.06"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.085"/>
                        <TextView
                            android:labelFor="@+id/nameEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:autoSizeMaxTextSize="30sp"
                            android:autoSizeMinTextSize="8sp"
                            android:autoSizeTextType="uniform"
                            android:paddingStart="8sp"
                            android:paddingEnd="8sp"
                            android:fontFamily="@font/helvetica_thin"
                            android:text="Nama"
                            android:textColor="#fff"
                            app:layout_constrainedHeight="true"
                            app:layout_constrainedWidth="true"
                            app:layout_constraintBottom_toTopOf="@id/cdhor2"
                            app:layout_constraintLeft_toLeftOf="@id/nameEdtx"
                            app:layout_constraintRight_toRightOf="@id/nameEdtx"
                            app:layout_constraintTop_toBottomOf="@id/cdhor1" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdver1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.075"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdver2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.9"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.095"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.13"/>
                        <EditText
                            android:id="@+id/nameEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_constrainedWidth="true"
                            android:autofillHints="text"
                            android:background="@drawable/backwhite"
                            android:fontFamily="@font/roboto_condensed_regular"
                            android:paddingHorizontal="8sp"
                            android:inputType="text"
                            app:layout_constrainedHeight="true"
                            app:layout_constraintTop_toBottomOf="@id/cdhor3"
                            app:layout_constraintLeft_toRightOf="@id/cdver1"
                            app:layout_constraintRight_toLeftOf="@id/cdver2"
                            app:layout_constraintBottom_toTopOf="@id/cdhor4"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.15"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.175"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdver3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.45"/>
                        <TextView
                            android:labelFor="@id/ageEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:paddingStart="8sp"
                            android:paddingEnd="8sp"
                            app:layout_constrainedHeight="true"
                            app:layout_constrainedWidth="true"
                            android:fontFamily="@font/helvetica_thin"
                            android:text="Umur"
                            android:textColor="#fff"
                            android:autoSizeTextType="uniform"
                            android:autoSizeMaxTextSize="30sp"
                            android:autoSizeMinTextSize="10sp"
                            app:layout_constraintTop_toBottomOf="@id/cdhor5"
                            app:layout_constraintLeft_toLeftOf="@id/ageEdtx"
                            app:layout_constraintBottom_toTopOf="@id/cdhor6"
                            app:layout_constraintRight_toRightOf="@id/ageEdtx" />
                        <TextView
                            android:labelFor="@id/genderEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:paddingStart="8sp"
                            android:paddingEnd="8sp"
                            app:layout_constrainedHeight="true"
                            app:layout_constrainedWidth="true"
                            android:fontFamily="@font/helvetica_thin"
                            android:text="Gender"
                            android:textColor="#fff"
                            android:autoSizeTextType="uniform"
                            android:autoSizeMaxTextSize="30sp"
                            android:autoSizeMinTextSize="10sp"
                            app:layout_constraintTop_toBottomOf="@id/cdhor5"
                            app:layout_constraintLeft_toLeftOf="@id/genderEdtx"
                            app:layout_constraintBottom_toTopOf="@id/cdhor6"
                            app:layout_constraintRight_toRightOf="@id/genderEdtx" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.185"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.225"/>
                        <EditText
                            android:id="@+id/ageEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:maxLength="2"
                            android:gravity="center"
                            app:layout_constrainedWidth="true"
                            android:autofillHints="email"
                            android:background="@drawable/backwhite"
                            android:fontFamily="@font/roboto_condensed_regular"
                            android:paddingHorizontal="8sp"
                            android:inputType="number"
                            app:layout_constrainedHeight="true"
                            app:layout_constraintTop_toBottomOf="@id/cdhor7"
                            app:layout_constraintLeft_toRightOf="@id/cdver1"
                            app:layout_constraintRight_toLeftOf="@id/cdver3"
                            app:layout_constraintBottom_toTopOf="@id/cdhor8"/>
                        <RadioGroup
                            android:id="@+id/genderEdtx"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            app:layout_constrainedWidth="true"
                            app:layout_constrainedHeight="true"
                            app:layout_constraintTop_toBottomOf="@id/cdhor7"
                            app:layout_constraintBottom_toTopOf="@id/cdhor8"
                            app:layout_constraintRight_toRightOf="@id/cdver2">
                            <androidx.appcompat.widget.AppCompatRadioButton
                                android:id="@+id/radio0"
                                android:gravity="center"
                                android:layout_width="wrap_content"
                                android:minWidth="55dp"
                                android:layout_height="match_parent"
                                android:layout_marginRight="2dp"
                                android:background="@drawable/cstmradio"
                                android:button="@drawable/cstmradio"
                                android:text="L" />
                            <androidx.appcompat.widget.AppCompatRadioButton
                                android:gravity="center"
                                android:id="@+id/radio1"
                                android:layout_width="wrap_content"
                                android:minWidth="55dp"
                                android:layout_marginLeft="2dp"
                                android:layout_height="match_parent"
                                android:background="@drawable/cstmradio"
                                android:button="@drawable/cstmradio"
                                android:text="P" />
                        </RadioGroup>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.245"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.27"/>
                        <TextView
                            android:labelFor="@id/hobbyEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:paddingStart="8sp"
                            android:paddingEnd="8sp"
                            app:layout_constrainedHeight="true"
                            app:layout_constrainedWidth="true"
                            android:fontFamily="@font/helvetica_thin"
                            android:text="Hobi atau kebiasaan"
                            android:textColor="#fff"
                            android:autoSizeTextType="uniform"
                            android:autoSizeMaxTextSize="30sp"
                            android:autoSizeMinTextSize="10sp"
                            app:layout_constraintTop_toBottomOf="@id/cdhor9"
                            app:layout_constraintLeft_toLeftOf="@id/hobbyEdtx"
                            app:layout_constraintBottom_toTopOf="@id/cdhor10"
                            app:layout_constraintRight_toRightOf="@id/hobbyEdtx" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor11"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.28"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor12"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.32"/>
                        <EditText
                            android:id="@+id/hobbyEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_constrainedWidth="true"
                            android:autofillHints="text"
                            android:background="@drawable/backwhite"
                            android:fontFamily="@font/roboto_condensed_regular"
                            android:paddingHorizontal="8sp"
                            android:inputType="textMultiLine"
                            app:layout_constrainedHeight="true"
                            app:layout_constraintTop_toBottomOf="@id/cdhor11"
                            app:layout_constraintLeft_toRightOf="@id/cdver1"
                            app:layout_constraintRight_toLeftOf="@id/cdver2"
                            app:layout_constraintBottom_toTopOf="@id/cdhor12"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor13"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.34"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor14"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.365"/>
                        <TextView
                            android:labelFor="@id/sickEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:paddingStart="8sp"
                            android:paddingEnd="8sp"
                            app:layout_constrainedHeight="true"
                            app:layout_constrainedWidth="true"
                            android:fontFamily="@font/helvetica_thin"
                            android:text="Riwayat penyakit"
                            android:textColor="#fff"
                            android:autoSizeTextType="uniform"
                            android:autoSizeMaxTextSize="30sp"
                            android:autoSizeMinTextSize="10sp"
                            app:layout_constraintTop_toBottomOf="@id/cdhor13"
                            app:layout_constraintLeft_toLeftOf="@id/hobbyEdtx"
                            app:layout_constraintBottom_toTopOf="@id/cdhor14"
                            app:layout_constraintRight_toRightOf="@id/hobbyEdtx" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor15"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.375"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor16"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.415"/>
                        <EditText
                            android:id="@+id/sickEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_constrainedWidth="true"
                            android:autofillHints="text"
                            android:background="@drawable/backwhite"
                            android:fontFamily="@font/roboto_condensed_regular"
                            android:paddingHorizontal="8sp"
                            android:inputType="textMultiLine"
                            app:layout_constrainedHeight="true"
                            app:layout_constraintTop_toBottomOf="@id/cdhor15"
                            app:layout_constraintLeft_toRightOf="@id/cdver1"
                            app:layout_constraintRight_toLeftOf="@id/cdver2"
                            app:layout_constraintBottom_toTopOf="@id/cdhor16"/>


                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor17"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.515"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor18"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.54"/>
                        <TextView
                            android:labelFor="@id/sickEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:paddingStart="8sp"
                            android:paddingEnd="8sp"
                            app:layout_constrainedHeight="true"
                            app:layout_constrainedWidth="true"
                            android:fontFamily="@font/helvetica_thin"
                            android:text="Tanggal mulai"
                            android:textColor="#000"
                            android:autoSizeTextType="uniform"
                            android:autoSizeMaxTextSize="30sp"
                            android:autoSizeMinTextSize="10sp"
                            app:layout_constraintTop_toBottomOf="@id/cdhor17"
                            app:layout_constraintLeft_toLeftOf="@id/hobbyEdtx"
                            app:layout_constraintBottom_toTopOf="@id/cdhor18"
                            app:layout_constraintRight_toRightOf="@id/hobbyEdtx" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor19"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.55"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor20"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.59"/>
                        <EditText
                            android:id="@+id/stdtEdtx"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_constrainedWidth="true"
                            android:autofillHints="text"
                            android:background="@drawable/borderblue"
                            android:fontFamily="@font/roboto_condensed_regular"
                            android:paddingHorizontal="8sp"
                            android:inputType="textMultiLine"
                            app:layout_constrainedHeight="true"
                            app:layout_constraintTop_toBottomOf="@id/cdhor19"
                            app:layout_constraintLeft_toRightOf="@id/cdver1"
                            app:layout_constraintRight_toLeftOf="@id/cdver2"
                            app:layout_constraintBottom_toTopOf="@id/cdhor20"/>
                        <TextView
                            android:id="@+id/stdtView"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:paddingStart="8sp"
                            android:paddingEnd="8sp"
                            app:layout_constrainedHeight="true"
                            app:layout_constrainedWidth="true"
                            android:textColor="#fff"
                            android:autoSizeTextType="uniform"
                            android:autoSizeMaxTextSize="30sp"
                            android:autoSizeMinTextSize="10sp"
                            app:layout_constraintTop_toTopOf="@id/stdtEdtx"
                            app:layout_constraintLeft_toLeftOf="@id/stdtEdtx"
                            app:layout_constraintBottom_toBottomOf="@id/stdtEdtx"
                            app:layout_constraintRight_toRightOf="@id/stdtEdtx" />

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor21"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.59"/>
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/cdhor22"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.62"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
